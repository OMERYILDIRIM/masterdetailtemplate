<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="masterdetailtemplate.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Master">
				<content>
					<Button text="To Detail" press="toDetail" />
					<List items="{/PersonelTelefonSet}">
						<ObjectListItem title="{Isim}"/>	
					</List>
				</content>
				<footer>
					<Toolbar>
						<ToolbarSpacer/>
						<Button icon="sap-icon://add" press="onCreate"/>
					</Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>